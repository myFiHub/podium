  "version": "0.2.0",
  "configurations": [


    {
      "name": "DEV",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define=ENVIRONMENT=development"]
    },
    {
      "name": "DEV-PROD",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define=ENVIRONMENT=production"]
    },
    {
      "name": "DEV-VERBOSE",
      "request": "launch",
      "type": "dart",
      "args": ["--verbose", "--dart-define=ENVIRONMENT=development"]
    },
    {
      "name": "RELEASE",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define=ENVIRONMENT=production"]
     },
    {
      "name": "PROFILE",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define=ENVIRONMENT=production"]
     },
  ]
}
