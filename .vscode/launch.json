  "version": "0.2.0",
  "configurations": [
    {
      "name": "Podium DEV",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file=env/dev.json"]
    },
    {
      "name": "Podium STAGE",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file=env/stage.json"]
    },
    {
      "name": "Podium PROD",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file=env/prod.json"]
    },
    {
      "name": "podium_flutter (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "podium_flutter (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
