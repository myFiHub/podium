<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>Podium</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>podium</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<!-- Deep Linking Configuration -->
		<key>CFBundleURLTypes</key>
		<array>
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLName</key>
				<string>com.web3podium</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>com.web3podium</string>
				</array>
			</dict>
		</array>

		<key>FlutterDeepLinkingEnabled</key>
		<false />

		<key>com.apple.developer.associated-domains</key>
		<array>
			<string>applinks:web3podium.page.link</string>
			<string>applinks:web3podium.com</string>
		</array>
		<!-- END Deep Linking Configuration -->


		<key>LSRequiresIPhoneOS</key>
		<true />
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<key>NSFaceIDUsageDescription</key>
		<string>We use Face ID for secure access to the app.</string>
		<key>NSCalendarsFullAccessUsageDescription</key>
		<string>Calendar full access</string>
		<!-- Camera -->
		<key>NSCameraUsageDescription</key>
		<string>Camera access is required for video calling</string>

		<!-- Microphone -->
		<key>NSMicrophoneUsageDescription</key>
		<string>Microphone access is required for voice calling</string>

		<!-- Photo Library -->
		<key>NSPhotoLibraryUsageDescription</key>
		<string>Photo library access is required for sharing images during meetings</string>

		<!-- Calendar -->
		<key>NSCalendarsUsageDescription</key>
		<string>Calendar access is required for scheduling meetings</string>

		<!-- Contacts -->
		<key>NSContactsUsageDescription</key>
		<string>Contacts access helps you easily invite people to meetings</string>

		<!-- Bluetooth -->
		<key>NSBluetoothPeripheralUsageDescription</key>
		<string>Bluetooth access is required for connecting to wireless headsets</string>

		<!-- Local Network -->
		<key>NSLocalNetworkUsageDescription</key>
		<string>Local network access is required for optimal connection quality</string>
		<key>UIMainStoryboardFile</key>
		<string>Main</string>
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>UISupportedInterfaceOrientations~ipad</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true />
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true />
		<key>UIStatusBarHidden</key>
		<false />
	</dict>
</plist>